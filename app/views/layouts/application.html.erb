<!DOCTYPE html>
<html>
<head>
  <title>Evaluation</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

<p id="navbar"><strong>Goober</strong> : 
  <% if current_user %>
    <%= link_to("My Page", home_page(current_user)) %> : <%= link_to("Log Out", logout_path) %>
  <% else %>
    <%= link_to("Log In", login_path) %>
  <% end %>
</p>

<%= yield %>

</body>
</html>
